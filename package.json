{
  "name": "icon-eu-downloader",
  "version": "1.0.1",
  "description": "IconEU downloader forecast data",
  "main": "dist/index.mjs",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.mjs",
    "dev": "nodemon --exec \"ts-node\" --esm src/index.mts",
    "lint": "eslint --fix --ext .mjs,.tsx ./",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true
    }
  },
  "lint-staged": {
    "**/*.{mjs,js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Virtuosofriend/icon-eu-downloader"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Virtuosofriend/icon-eu-downloader/issues"
  },
  "homepage": "https://github.com/Virtuosofriend/icon-eu-downloader#readme",
  "dependencies": {
    "axios": "1.7.4",
    "dotenv": "16.4.5"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.13.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.10",
    "nodemon": "3.1.7",
    "standard-version": "9.5.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
